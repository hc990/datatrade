</page>

	<page id="/member.html">
		<widget id="1">
			<overflow>visible</overflow>
			<border/>
			<type>member_main</type>
		</widget>
	</page>

	<page id="/article.html">
		<widget id="3">
			<cat_id>2</cat_id>
			<overflow>visible</overflow>
			<border/>
			<type>articlecat</type>
		</widget>
		<widget id="2">
			<overflow>visible</overflow>
			<border/>
			<type>shop_article</type>
		</widget>
		<widget id="5">
			<object_type>discuss</object_type>
			<overflow>visible</overflow>
			<border/>
			<type>goods_comments</type>
			<commentType>article</commentType>
		</widget>
	</page>

	<page id="/help.html">
		<widget id="3">
			<overflow>visible</overflow>
			<border>border2</border>
			<bordertitle>帮助中心</bordertitle>
			<cat_id>1</cat_id>
			<type>articlecat</type>
		</widget>
		<widget id="2">
			<overflow>visible</overflow>
			<border/>
			<type>shop_help</type>
		</widget>
	</page>


	<page id="/articleList.html">
		<widget id="2">
			<overflow>visible</overflow>
			<border/>
			<cat_id>7</cat_id>
			<showpage>1</showpage>
			<showdate>1</showdate>
			<showmore>0</showmore>
			<type>articleList</type>
		</widget>

		
		<widget id="3">
			<overflow>visible</overflow>
			<border>border3</border>
			<bordertitle>新闻中心</bordertitle>
			<cat_id>2</cat_id>
			<type>articlecat</type>
		</widget>
		
	</page>



	<page id="/cart.html">

		<widget id="1">
			<overflow>visible</overflow>
			<border/>
			<type>shop_cart</type>
		</widget>

	</page>

	<page id="/default.html">
		<widget id="2">
			<overflow>visible</overflow>
			<border>border2</border>
			<bordertitle>商品分类</bordertitle>
			<type>goods_cat</type>
		</widget>
		<widget id="/goods-(\d+).html">
			<overflow>visible</overflow>
			<border/>
			<type>goods_detail</type>
		</widget>
		<widget id="/search-(.*).html">
			<overflow>visible</overflow>
			<border/>
			<type>goods_search</type>
		</widget>
		<widget id="/compare.html">
			<overflow>visible</overflow>
			<border/>
			<type>goods_compare</type>
		</widget>
		<widget id="/package.html">
			<overflow>visible</overflow>
			<border/>
			<type>goods_compare</type>
		</widget>
		<widget id="/brandDetail-(\d+).html">
			<overflow>visible</overflow>
			<border/>
			<type>brand_detail</type>
		</widget>
		<widget id="/brand.html">
			<overflow>visible</overflow>
			<border/>
			<type>brand_list</type>
		</widget>


		<widget id="/leavewords.html">
 
			<overflow>visible</overflow>
			<border/>
			<type>shop_guestbook</type>
 
		</widget>

		<widget id="/friendslink.html">
			<overflow>visible</overflow>
			<border/>
			<cat_id>1</cat_id>
			<type>friends_link</type>
		</widget>
		<widget id="/package.html">
			<overflow>visible</overflow>
			<border/>
			<cat_id>1</cat_id>
			<type>package</type>
		</widget>
		<widget id="/giftlist.html">
			<overflow>visible</overflow>
			<showtype>list</showtype>
			<border/>
			<type>gift_list</type>
		</widget>
		<widget id="/gift-(\d+).html">
			<overflow>visible</overflow>
			<showtype>detail</showtype>
			<border/>
			<type>gift_list</type>
		</widget>
	</page>


</widgets>
