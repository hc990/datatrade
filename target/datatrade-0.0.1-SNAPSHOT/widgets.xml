			<border>none</border>
			<type>partZone</type>
		</widget>

	</page>


	<page id="/index.html">

<widget id="2">
<overflow>visible</overflow>
<bordertitle>商品分类</bordertitle>
<border>border2</border>
<type>goods_cat</type>
</widget>

<widget id="3">
<bordertitle>特价商品</bordertitle>
	<border>border1</border>
	<type>goods_list</type>
	<term>{tagid:'1'}</term>
	<setting>{type:'default',goodsImgPosition:'top', thumbnail_pic_width:'115px',thumbnail_pic_height:'160px', showGoodsName:'on', goodsNum:5, columnnum:5}</setting>
	<overflow>visible</overflow>
</widget>
<widget id="4">
<cat_id>7</cat_id>
<overflow>visible</overflow>
<bordertitle>网店公告</bordertitle>
<border>border2</border>
<type>articleList</type>
<showdate>0</showdate>
<showpage>0</showpage>
<showmore>1</showmore>
</widget>
<widget id="5">
<acid>6</acid>
<overflow>visible</overflow>
<bordertitle/>
<border>none</border>
<type>ad</type>
</widget>
<widget id="6">
<bordertitle>推荐商品</bordertitle>
	<border>border1</border>
	<type>goods_list</type>
	<term>{tagid:'3'}</term>
	<setting>{type:'default',goodsImgPosition:'top', thumbnail_pic_width:'120px',thumbnail_pic_height:'156px', showGoodsName:'on', goodsNum:10, columnnum:5}</setting>
	<overflow>visible</overflow>
</widget>
<widget id="7">
<cat_id>7</cat_id>
<overflow>visible</overflow>
<bordertitle>常见问题</bordertitle>
