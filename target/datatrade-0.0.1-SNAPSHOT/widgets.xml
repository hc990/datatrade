<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<widgets>
	<page id="common">
		<widget id="header">
			<type>header</type>
			<border>none</border>
		</widget>

		<widget id="mloginbar">
			<type>member_login_bar</type>
			<border>none</border>
		</widget>

		<widget id="mcartbar">
			<type>member_cart_bar</type>
			<border>none</border>
		</widget>

		<widget id="menu">
			<type>shop_menu</type>
			<border>none</border>
		</widget>

		<widget id="searchbar">
			<type>searchBar</type>
			<border>none</border>
		</widget>
		
		<widget id="logo">
			<type>imageWidget</type>
			<src>attachment/images/logo.png</src>
		</widget>
		<widget id="banner">
			<type>ad</type>
			<acid>1</acid>
		</widget>
		<widget id="tel">
